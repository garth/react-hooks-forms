{
  "name": "react-hooks-forms",
  "version": "0.2.3",
  "description": "A react hooks approach to forms",
  "repository": "https://github.com/garth/react-hooks-forms.git",
  "author": "Garth Williams",
  "license": "MIT",
  "keywords": [
    "react",
    "form",
    "forms",
    "hook",
    "hooks",
    "use"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prebuild": "rimraf lib && rimraf es",
    "build:lib": "tsc --outDir lib --target es5 --preserveWatchOutput",
    "build:es": "tsc --outDir es --module es2015 --preserveWatchOutput",
    "build": "npm run build:lib & npm run build:es",
    "lint": "tslint -c tslint.json -t verbose 'src/**/*.{ts,tsx}'",
    "prettier": "prettier --write \"src/**/*.{js,json,ts,tsx,scss,html}\""
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxBracketSameLine": true
  },
  "devDependencies": {
    "@types/react": "^16.9.34",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.2.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "tslib": "^1.11.1"
  }
}
